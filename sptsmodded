menuFrame.Position = UDim2.new(0.5, -150, 0.5, -200)  -- Merkeze yerleştirdik
menuFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
menuFrame.BackgroundTransparency = 0.5  -- Şeffaf yapalım
menuFrame.Visible = false
menuFrame.Parent = screenGui

