toggleButton.TextScaled = true
toggleButton.Parent = screenGui

-- Info menüsü için ayrı bir çerçeve
local infoFrame = Instance.new("Frame")
infoFrame.Size = UDim2.new(0, 200, 0, 200)  -- Ufak bir panel
infoFrame.Position = UDim2.new(0.5, -250, 0.5, -150)  -- Menüye göre konumlandırdık
infoFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
infoFrame.BackgroundTransparency = 0.5
infoFrame.Visible = false
infoFrame.Parent = screenGui

-- Info içeriği (TextLabel) ekleyelim
local infoText = Instance.new("TextLabel")
infoText.Size = UDim2.new(0, 200, 0, 40)
infoText.Position = UDim2.new(0.5, -100, 0.1, 0)
infoText.Text = "Beta 1.01\nNoLag Fix\nMenu Update\nCreated By Massthix."
infoText.TextColor3 = Color3.fromRGB(255, 255, 255)
infoText.BackgroundTransparency = 1  -- Şeffaf
infoText.TextScaled = true
infoText.Parent = infoFrame

-- Menü açma/kapama işlevi
toggleButton.MouseButton1Click:Connect(function()
    menuFrame.Visible = not menuFrame.Visible
    if menuFrame.Visible then
        toggleButton.Text = "Menüyü Kapat"
    else
        toggleButton.Text = "Menüyü Aç"
    end
end)

-- Karakter öldüğünde menünün kaybolmasını engelle
local function onCharacterAdded(character)
    task.wait(1) -- Karakter tam yüklensin diye biraz bekletiyoruz
    screenGui.Parent = game:GetService("CoreGui") -- Tekrar CoreGui içine ekliyoruz
end

player.CharacterAdded:Connect(onCharacterAdded)

-- Eğer oyuncunun karakteri varsa, hemen çalıştır
if player.Character then
    onCharacterAdded(player.Character)
end


-- Info butonuna tıklanınca Info menüsünü göster
infoButton.MouseButton1Click:Connect(function()
    infoFrame.Visible = not infoFrame.Visible
end)

-- Fist scriptini çalıştıran fonksiyon
local function activateFist()
    _G.cum = true
    local args = {
        [1] = {
            [1] = "Fist",
            [2] = "Fist1"
        }
    }
    game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))

    while _G.cum do
        local args = {
            [1] = {
                [1] = "+FS21"
            }
        }
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
        wait(0.0001)
        local args = {
            [1] = {
                [1] = ""
            }
        }
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
        task.wait()
    end

    if _G.cum == false then
        local args = {
            [1] = {
                [1] = "",
                [2] = ""
            }
        }
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
    end
end

-- Fist butonuna tıklanınca fist scriptini çalıştır
fistButton.MouseButton1Click:Connect(function()
    activateFist()
end)

-- Anti AFK scriptini çalıştıran fonksiyon
local function activateAntiAfk()
    wait(0.5)
    local ba = Instance.new("ScreenGui")
    local ca = Instance.new("TextLabel")
    local da = Instance.new("Frame")
    local _b = Instance.new("TextLabel")
    local ab = Instance.new("TextLabel")
    ba.Parent = game.CoreGui
    ba.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    ca.Parent = ba
    ca.Active = true
    ca.BackgroundColor3 = Color3.new(0.176471, 0.176471, 0.176471)
    ca.Draggable = true
    ca.Position = UDim2.new(0.698610067, 0, 0.098096624, 0)
    ca.Size = UDim2.new(0, 370, 0, 52)
    ca.Font = Enum.Font.SourceSansSemibold
    ca.Text = "Anti AFK Script"
    ca.TextColor3 = Color3.new(0, 1, 1)
    ca.TextSize = 22
    da.Parent = ca
    da.BackgroundColor3 = Color3.new(0.196078, 0.196078, 0.196078)
    da.Position = UDim2.new(0, 0, 1.0192306, 0)
    da.Size = UDim2.new(0, 370, 0, 107)
    _b.Parent = da
    _b.BackgroundColor3 = Color3.new(0.176471, 0.176471, 0.176471)
    _b.Position = UDim2.new(0, 0, 0.800455689, 0)
    _b.Size = UDim2.new(0, 370, 0, 21)
    _b.Font = Enum.Font.Arial
    _b.Text = "Made by Bab#0325"
    _b.TextColor3 = Color3.new(0, 1, 1)
    _b.TextSize = 20
    ab.Parent = da
    ab.BackgroundColor3 = Color3.new(0.176471, 0.176471, 0.176471)
    ab.Position = UDim2.new(0, 0, 0.158377, 0)
    ab.Size = UDim2.new(0, 370, 0, 44)
    ab.Font = Enum.Font.ArialBold
    ab.Text = "Status: Active"
    ab.TextColor3 = Color3.new(0, 1, 1)
    ab.TextSize = 20
    local bb = game:service'VirtualUser'
    game:service'Players'.LocalPlayer.Idled:connect(function()
        bb:CaptureController()
        bb:ClickButton2(Vector2.new())
        ab.Text = "Roblox Tried to kick you but we didnt let them kick you :D"
        wait(2)
        ab.Text = "Status : Active"
    end)
end

-- Anti AFK butonuna tıklanınca anti afk scriptini çalıştır
antiAfkButton.MouseButton1Click:Connect(function()
    activateAntiAfk()
end)

-- Autodeathrun scriptini çalıştıran fonksiyon
local function activateAutodeathrun()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sinret/rbxscript.com-scripts-reuploads-/main/spts", true))()
end

-- Autodeathrun butonuna tıklanınca autodeathrun scriptini çalıştır
autodeathrunButton.MouseButton1Click:Connect(function()
    activateAutodeathrun()
end)

